{
    "manifest_version": 2,

    "name": "BS API Plugin",
    "description": "This extension will launch BS interface",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*"
    ]
}
